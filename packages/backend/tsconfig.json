{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "downlevelIteration": true,
    "strictNullChecks": false,
    "plugins": [
      { "transform": "ts-interface-keys-transformer/transformer" },
      { "transform": "ts-reflection/transformer" }
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
